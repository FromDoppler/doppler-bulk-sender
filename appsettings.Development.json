{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppConfiguration": {
    "smtpHost": "smtp.dopplerrelay.com",
    "smtpPort": 587,
    "BaseUrl": "http://api.dopplerrelay.com/",
    "AccountUrl": "/accounts/{AccountId}",
    "TemplateUrl": "/accounts/{AccountId}/templates/{TemplateId}/message",
    "FtpListInterval": 600000,
    "ReportsInterval": 60000,
    "CleanInterval": 600000,
    "PreProcessorInterval": 60000,
    "LocalFilesInterval": 60000,
    "LocalDownloadFolder": "c:\\temp\\localfile",
    "DeliveryInterval": 100,
    "DeliveryRetryCount": 3,
    "MaxNumberOfThreads": 2,
    "BulkEmailCount": 100,
    "CleanDays": 30,
    "CleanAttachmentsDays": 5,
    "AdminUser": "",
    "AdminPass": ""
  }
}
